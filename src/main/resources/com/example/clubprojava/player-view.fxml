<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.clubprojava.controller.PlayerController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="titleText"/>
    <Label fx:id="clubName"/>
    <TableView fx:id="playerTable" layoutX="14.0" layoutY="14.0" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="firstNameColumn" text="Prénom" prefWidth="100.0"/>
            <TableColumn fx:id="lastNameColumn" text="Nom" prefWidth="100.0"/>
            <TableColumn fx:id="positionColumn" text="Poste" prefWidth="100.0"/>
            <TableColumn fx:id="ageColumn" text="Âge" prefWidth="100.0"/>
        </columns>
    </TableView>

</VBox>
